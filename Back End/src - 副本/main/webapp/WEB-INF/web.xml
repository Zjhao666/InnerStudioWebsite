<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <!-->login<-->
    <servlet>
        <servlet-name>Regis</servlet-name>
        <servlet-class>login.Regis</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Regis</servlet-name>
        <url-pattern>/regis.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>login.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>
    <!-->home<-->
    <servlet>
        <servlet-name>swPlan</servlet-name>
        <servlet-class>home.swPlan</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swPlan</servlet-name>
        <url-pattern>/swPlan.get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>swProjectList</servlet-name>
        <servlet-class>home.swProjectList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swProjectList</servlet-name>
        <url-pattern>/swProjectList.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>swProjectDetail</servlet-name>
        <servlet-class>home.swProjectDetail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swProjectDetail</servlet-name>
        <url-pattern>/swProjectDetail.do?id=</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>swMemberList</servlet-name>
        <servlet-class>home.swMemberList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swMemberList</servlet-name>
        <url-pattern>/swMemberList.get</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>swNotAuditedProjectList</servlet-name>
        <servlet-class>home.swNotAuditedProjectList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swNotAuditedProjectList</servlet-name>
        <url-pattern>/swNotAuditedProjectList.do</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>swUpdateProject</servlet-name>
        <servlet-class>home.swUpdateProject</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swUpdateProject</servlet-name>
        <url-pattern>/swUpdateProject.do</url-pattern>
    </servlet-mapping>
    
    
    <!-->personal<-->
    <servlet>
        <servlet-name>swMemberHeadImg</servlet-name>
        <servlet-class>personal.swMemberHeadImg</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swMemberHeadImg</servlet-name>
        <url-pattern>/swMemberHeadImg.get</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>swPrivatePage</servlet-name>
        <servlet-class>personal.swPrivatePage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>swPrivatePage</servlet-name>
        <url-pattern>/swPrivatePage.html</url-pattern>
    </servlet-mapping>
</web-app>
